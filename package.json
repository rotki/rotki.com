{
  "name": "rotki.com",
  "version": "1.37.2",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "type": "module",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi typecheck && nuxi build",
    "clean": "rimraf node_modules .output .nuxt dist && pnpm store prune && pnpm i",
    "start": "nuxi start",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint:all": "pnpm lint && pnpm lint:style",
    "typecheck": "npx nuxi typecheck",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "release": "bumpp -r --no-push"
  },
  "optionalDependencies": {
    "cypress": "14.5.4"
  },
  "dependencies": {
    "@nuxtjs/robots": "5.5.0",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@pinia/nuxt": "0.11.2",
    "@reown/appkit": "1.8.1",
    "@reown/appkit-adapter-ethers": "1.8.1",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "13.7.0",
    "@vueuse/math": "13.7.0",
    "@vueuse/nuxt": "13.7.0",
    "@vueuse/shared": "13.7.0",
    "better-sqlite3": "12.2.0",
    "braintree-web": "3.126.0",
    "ethers": "6.15.0",
    "pinia": "3.0.3",
    "qrcode": "1.5.4",
    "swiper": "11.2.10",
    "vue": "3.5.18",
    "vue-router": "4.5.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@fontsource/roboto": "5.2.6",
    "@nuxt/content": "3.6.3",
    "@nuxt/devtools": "2.6.2",
    "@nuxt/image": "1.11.0",
    "@nuxt/test-utils": "3.19.2",
    "@nuxtjs/i18n": "10.0.6",
    "@nuxtjs/sitemap": "7.4.3",
    "@rotki/eslint-config": "4.4.0",
    "@rotki/eslint-plugin": "1.1.0",
    "@rotki/ui-library": "2.6.3",
    "@types/braintree-web": "3.96.17",
    "@types/paypal-checkout-components": "4.0.8",
    "@types/qrcode": "1.5.5",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "10.4.21",
    "bumpp": "10.2.3",
    "eslint": "9.33.0",
    "eslint-plugin-nuxt": "4.0.0",
    "happy-dom": "18.0.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.5",
    "msw": "2.10.5",
    "nuxt": "3.18.1",
    "postcss": "8.5.6",
    "postcss-html": "1.8.0",
    "postcss-import": "16.1.1",
    "postcss-url": "10.1.3",
    "rollup": "4.46.4",
    "sass": "1.90.0",
    "sass-loader": "16.0.5",
    "stylelint": "16.23.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.0",
    "stylelint-config-standard-scss": "15.0.1",
    "stylelint-order": "7.0.0",
    "stylelint-scss": "6.12.1",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "vite": "7.1.3",
    "vitest": "3.2.4",
    "vitest-environment-nuxt": "1.0.1",
    "vue-tsc": "3.0.5",
    "webpack": "5.101.3"
  },
  "engines": {
    "node": ">=22 <23",
    "pnpm": ">=10 <11"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "cypress",
      "msw",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "vue-demi"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml}": "eslint",
    "*.{css,vue}": "stylelint"
  }
}
