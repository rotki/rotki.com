{
  "name": "rotki.com",
  "version": "1.30.3",
  "private": true,
  "packageManager": "pnpm@10.7.1",
  "type": "module",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi typecheck && nuxi build",
    "clean": "rimraf node_modules .output .nuxt dist && pnpm store prune && pnpm i",
    "start": "nuxi start",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint:all": "pnpm lint && pnpm lint:style",
    "typecheck": "npx nuxi typecheck",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "release": "bumpp -r --no-push"
  },
  "optionalDependencies": {
    "cypress": "14.2.1"
  },
  "dependencies": {
    "@metamask/detect-provider": "2.0.0",
    "@nuxtjs/robots": "5.2.8",
    "@nuxtjs/tailwindcss": "6.13.2",
    "@pinia/nuxt": "0.10.1",
    "@reown/appkit": "1.7.2",
    "@reown/appkit-adapter-ethers": "1.7.2",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "13.0.0",
    "@vueuse/math": "13.0.0",
    "@vueuse/nuxt": "13.0.0",
    "@vueuse/shared": "13.0.0",
    "braintree-web": "3.117.1",
    "ethers": "6.13.6",
    "pinia": "3.0.1",
    "qrcode": "1.5.4",
    "swiper": "11.2.6",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@fontsource/roboto": "5.2.5",
    "@nuxt/content": "3.4.0",
    "@nuxt/devtools": "2.3.2",
    "@nuxt/image": "1.10.0",
    "@nuxt/test-utils": "3.17.2",
    "@nuxtjs/i18n": "9.4.0",
    "@nuxtjs/sitemap": "7.2.9",
    "@rotki/eslint-config": "4.1.0",
    "@rotki/eslint-plugin": "1.0.0",
    "@rotki/ui-library": "1.12.3",
    "@types/braintree-web": "3.96.17",
    "@types/paypal-checkout-components": "4.0.8",
    "@types/qrcode": "1.5.5",
    "@vitest/coverage-v8": "3.1.1",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "10.4.21",
    "bumpp": "10.1.0",
    "eslint": "9.23.0",
    "eslint-plugin-nuxt": "4.0.0",
    "happy-dom": "17.4.4",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "msw": "2.7.3",
    "nuxt": "3.16.2",
    "postcss": "8.5.3",
    "postcss-html": "1.8.0",
    "postcss-import": "16.1.0",
    "postcss-url": "10.1.3",
    "rollup": "4.39.0",
    "sass": "1.86.2",
    "sass-loader": "16.0.5",
    "stylelint": "16.17.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.11.1",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.2",
    "vite": "6.2.7",
    "vitest": "3.1.1",
    "vitest-environment-nuxt": "1.0.1",
    "vue-tsc": "2.2.8",
    "webpack": "5.98.0"
  },
  "engines": {
    "node": ">=22 <23",
    "pnpm": ">=10 <11"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "cypress",
      "msw",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "vue-demi"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml}": "eslint",
    "*.{css,vue}": "stylelint"
  }
}
