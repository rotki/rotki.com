{
  "name": "rotki.com",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.2.0",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "nuxi start",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint:all": "pnpm lint && pnpm lint:style",
    "test": "vitest run",
    "test:watch": "vitest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "release": "bumpp -r --no-push"
  },
  "dependencies": {
    "@metamask/detect-provider": "1.2.0",
    "@nuxtjs/robots": "3.0.0",
    "@nuxtjs/tailwindcss": "6.6.6",
    "@pinia/nuxt": "0.4.8",
    "@vuelidate/core": "2.0.2",
    "@vuelidate/validators": "2.0.2",
    "@vueuse/core": "9.13.0",
    "@vueuse/nuxt": "9.13.0",
    "@vueuse/shared": "9.13.0",
    "braintree-web": "3.92.1",
    "ethers": "5.7.2",
    "loglevel": "1.8.1",
    "pinia": "2.0.34",
    "qrcode": "1.5.1",
    "vue": "3.2.47",
    "vue-router": "4.1.6",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.0",
    "@commitlint/config-conventional": "17.6.0",
    "@nuxt/content": "2.5.2",
    "@nuxt/devtools": "0.4.0",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@rotki/eslint-config": "1.0.4",
    "@types/braintree-web": "3.75.23",
    "@types/paypal-checkout-components": "4.0.5",
    "@types/qrcode": "1.5.0",
    "@vitest/coverage-c8": "0.30.1",
    "autoprefixer": "10.4.14",
    "bumpp": "9.1.0",
    "cypress": "12.9.0",
    "eslint": "8.38.0",
    "eslint-plugin-nuxt": "4.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "msw": "1.2.1",
    "nuxt": "3.3.2",
    "nuxt-simple-sitemap": "2.4.11",
    "nuxt-vitest": "0.6.9",
    "postcss": "8.4.21",
    "postcss-html": "1.5.0",
    "postcss-import": "15.1.0",
    "postcss-url": "10.1.3",
    "prettier": "2.8.7",
    "sass": "1.62.0",
    "sass-loader": "13.2.2",
    "stylelint": "15.4.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "32.0.0",
    "stylelint-config-standard-scss": "8.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-scss": "4.6.0",
    "tailwindcss": "3.3.1",
    "typescript": "4.9.5",
    "vite": "4.2.1",
    "vitest": "0.30.1",
    "webpack": "5.79.0"
  },
  "engines": {
    "node": ">=18 <19",
    "pnpm": ">=8 <9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  }
}
