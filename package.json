{
  "name": "rotki.com",
  "version": "1.3.0",
  "private": true,
  "packageManager": "pnpm@8.6.1",
  "type": "module",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi typecheck && nuxi build",
    "start": "nuxi start",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint:all": "pnpm lint && pnpm lint:style",
    "test": "vitest run",
    "test:watch": "vitest",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "release": "bumpp -r --no-push"
  },
  "dependencies": {
    "@metamask/detect-provider": "2.0.0",
    "@nuxtjs/robots": "3.0.0",
    "@nuxtjs/tailwindcss": "6.7.0",
    "@pinia/nuxt": "0.4.11",
    "@vuelidate/core": "2.0.2",
    "@vuelidate/validators": "2.0.2",
    "@vueuse/core": "10.1.2",
    "@vueuse/nuxt": "10.1.2",
    "@vueuse/shared": "10.1.2",
    "braintree-web": "3.94.0",
    "ethers": "6.4.0",
    "loglevel": "1.8.1",
    "pinia": "2.1.3",
    "qrcode": "1.5.3",
    "vue": "3.3.4",
    "vue-router": "4.2.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@nuxt/content": "2.6.0",
    "@nuxt/devtools": "0.5.5",
    "@nuxtjs/i18n": "8.0.0-beta.12",
    "@rotki/eslint-config": "1.0.6",
    "@types/braintree-web": "3.75.23",
    "@types/paypal-checkout-components": "4.0.5",
    "@types/qrcode": "1.5.0",
    "@vitest/coverage-c8": "0.31.1",
    "autoprefixer": "10.4.14",
    "bumpp": "9.1.0",
    "cypress": "12.13.0",
    "eslint": "8.41.0",
    "eslint-plugin-nuxt": "4.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "msw": "1.2.1",
    "nuxt": "3.5.1",
    "nuxt-simple-sitemap": "2.6.1",
    "nuxt-vitest": "0.7.1",
    "postcss": "8.4.23",
    "postcss-html": "1.5.0",
    "postcss-import": "15.1.0",
    "postcss-url": "10.1.3",
    "prettier": "2.8.8",
    "sass": "1.62.1",
    "sass-loader": "13.3.0",
    "stylelint": "15.6.2",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-config-standard-scss": "9.0.0",
    "stylelint-order": "6.0.3",
    "stylelint-scss": "5.0.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4",
    "vite": "4.3.8",
    "vitest": "0.31.1",
    "vue-tsc": "1.7.8",
    "webpack": "5.84.0"
  },
  "engines": {
    "node": ">=18 <19",
    "pnpm": ">=8 <9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  }
}
