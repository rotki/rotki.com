{
  "name": "rotki.com",
  "version": "1.27.1",
  "private": true,
  "packageManager": "pnpm@9.15.5",
  "type": "module",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi typecheck && nuxi build",
    "clean": "rimraf node_modules .output .nuxt dist && pnpm store prune && pnpm i",
    "start": "nuxi start",
    "generate": "nuxi generate",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore",
    "lint:all": "pnpm lint && pnpm lint:style",
    "typecheck": "npx nuxi typecheck",
    "test": "vitest run",
    "test:watch": "vitest --ui",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "release": "bumpp -r --no-push"
  },
  "optionalDependencies": {
    "cypress": "14.0.2"
  },
  "dependencies": {
    "@metamask/detect-provider": "2.0.0",
    "@nuxtjs/robots": "5.2.2",
    "@nuxtjs/tailwindcss": "6.11.4",
    "@pinia/nuxt": "0.9.0",
    "@reown/appkit": "1.6.8",
    "@reown/appkit-adapter-ethers": "1.6.8",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "12.5.0",
    "@vueuse/math": "12.5.0",
    "@vueuse/nuxt": "12.5.0",
    "@vueuse/shared": "12.5.0",
    "braintree-web": "3.115.1",
    "ethers": "6.13.5",
    "pinia": "2.3.1",
    "qrcode": "1.5.4",
    "swiper": "11.2.2",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@fontsource/roboto": "5.1.1",
    "@nuxt/content": "3.1.1",
    "@nuxt/devtools": "2.0.0",
    "@nuxt/image": "1.9.0",
    "@nuxt/test-utils": "3.15.4",
    "@nuxtjs/i18n": "9.2.0",
    "@nuxtjs/sitemap": "7.2.4",
    "@rotki/eslint-config": "3.7.1",
    "@rotki/eslint-plugin": "0.7.0",
    "@rotki/ui-library": "1.12.1",
    "@types/braintree-web": "3.96.16",
    "@types/paypal-checkout-components": "4.0.8",
    "@types/qrcode": "1.5.5",
    "@vitest/coverage-v8": "3.0.5",
    "@vue/test-utils": "2.4.6",
    "autoprefixer": "10.4.20",
    "bumpp": "10.0.3",
    "eslint": "9.20.0",
    "eslint-plugin-nuxt": "4.0.0",
    "happy-dom": "16.8.1",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "msw": "2.7.0",
    "nuxt": "3.15.4",
    "postcss": "8.5.2",
    "postcss-html": "1.8.0",
    "postcss-import": "16.1.0",
    "postcss-url": "10.1.3",
    "rollup": "4.34.6",
    "sass": "1.84.0",
    "sass-loader": "16.0.4",
    "stylelint": "16.14.1",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "37.0.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.11.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.3",
    "vite": "6.1.0",
    "vitest": "3.0.5",
    "vitest-environment-nuxt": "1.0.1",
    "vue-tsc": "2.1.10",
    "webpack": "5.97.1"
  },
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=9 <10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml}": "eslint",
    "*.{css,vue}": "stylelint"
  }
}
